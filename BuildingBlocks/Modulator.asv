classdef Modulator
    properties
        samplesPerSymbol=16
        fc
    end
    methods
        function obj =Modulator(fc, samplesPerSymbol)
            obj.samplesPerSymbol=samplesPerSymbol;
            obj.fc=fc;
        end
        function retSignal = step(obj, signal)
        end
        
        function retSignal= pulseShape(obj, signal)
        end
    end
end